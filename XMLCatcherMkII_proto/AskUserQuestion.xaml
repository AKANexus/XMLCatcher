<Window x:Class="XMLCatcherMkII_Installer.AskUserQuestion"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:XMLCatcherMkII_Installer"
        mc:Ignorable="d"
        Title="AskUserQuestion" Height="64" Width="489" WindowStyle="None" ResizeMode="NoResize" KeyDown="Window_KeyDown">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <TextBlock HorizontalAlignment="Left" Text="TextBlock" TextWrapping="Wrap" VerticalAlignment="Center" Margin="5" Name="tbl_Titulo"/>

        <PasswordBox HorizontalAlignment="Center" Grid.Row="1" VerticalAlignment="Top" Margin="5" Width="469" Name="txb_UserResponse" KeyDown="txb_UserResponse_KeyDown"/>

    </Grid>
</Window>
